        }, 6000);

        setTimeout(function(){

            sendPcsData(0);
        }, 7000);
        setTimeout(function(){

            sendPcsData(1);
        }, 8000);

        setTimeout(function(){

            sendPcsData(0);
        }, 9000);
        setTimeout(function(){

            sendPcsData(1);
        }, 10000);


        //Battery
        setTimeout(function(){


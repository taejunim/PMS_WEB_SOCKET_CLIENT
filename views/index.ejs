        }, 6000);

        setTimeout(function(){

            sendBatteryData(2);
        }, 7000);

        setTimeout(function(){

            sendBatteryData(1);
        }, 8000);

        setTimeout(function(){

            sendBatteryData(0);
        }, 9000);

        setTimeout(function(){

            sendBatteryData(4);
        }, 10000);
    }

    function sendOperationMonitoringData() {
        setTimeout(function(){

            sendMonitoringData(0);
        }, 1000);


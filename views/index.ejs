<script>
    var ws = new WebSocket('ws://localhost:3100');
    //var ws = new WebSocket('ws://101.101.216.193:3100');

    ws.onopen = (event) => {
        console.log("---------------------------------------------");
        let sendData = {id: 'M/W2', eventType: 'req', deviceType: 'middleware', dataType: 'connect'};
        ws.send(JSON.stringify(sendData));
        console.log("---------------------------------------------\n\n");
    }

    ws.onmessage = (event) => {
        console.log("---------------------------------------------");
        console.log("M/W2 : Data Received From Server");

        let receivedData = JSON.parse(event.data);

        switch (receivedData.eventType) {

            case 'req' :

                switch (receivedData.dataType) {

                    case 'status':
                        console.log('              status                ');
                        console.log(event.data);
                        console.log('M/W2 id : ' + receivedData.id);
                        console.log('M/W2 eventType : ' + receivedData.eventType);
                        console.log('M/W2 dataType : ' + receivedData.dataType);
                        console.log('M/W2 result : ' + receivedData.result);
                        console.log('M/W2 message : ' + receivedData.message);
                        break;

                    case 'control':
                        console.log('              control                ');
                        console.log(event.data);
                        console.log('M/W2 id : ' + receivedData.id);
                        console.log('M/W2 eventType : ' + receivedData.eventType);
                        console.log('M/W2 deviceType : ' + receivedData.deviceType);
                        console.log('M/W2 dataType : ' + receivedData.dataType);
                        console.log('M/W2 data : ' + receivedData.data);
                        console.log('M/W2 targetId : ' + receivedData.data.targetId);
                        console.log('M/W2 controlType : ' + receivedData.data.controlType);
                        console.log('M/W2 controlValue : ' + receivedData.data.controlValue);

                        let sendData = {
                            id: receivedData.id,
                            eventType: 'res',
                            dataType: receivedData.dataType,
                            result: 'success',
                            message: ''
                        };

                        console.log("---------------------------------------------");
                        console.log("sendData : " + JSON.stringify(sendData));
                        console.log("---------------------------------------------\n\n");

                        ws.send(JSON.stringify(sendData));

                        break;
                    default:
                }

                break;

            case 'res' :

                switch (receivedData.dataType) {
                    case 'connect':
                        console.log('              connect                ');
                        console.log(event.data);
                        console.log('M/W2 id : ' + receivedData.id);
                        console.log('M/W2 eventType : ' + receivedData.eventType);
                        console.log('M/W2 dataType : ' + receivedData.dataType);
                        console.log('M/W2 result : ' + receivedData.result);
                        console.log('M/W2 message : ' + receivedData.message);
                        break;

                    case 'status':
                        console.log('              status                ');
                        console.log(event.data);
                        console.log('M/W2 id : ' + receivedData.id);
                        console.log('M/W2 eventType : ' + receivedData.eventType);
                        console.log('M/W2 dataType : ' + receivedData.dataType);
                        console.log('M/W2 result : ' + receivedData.result);
                        console.log('M/W2 message : ' + receivedData.message);
                        break;

                    case 'control':
                        console.log('              control                ');
                        console.log(event.data);
                        console.log('M/W2 id : ' + receivedData.id);
                        console.log('M/W2 eventType : ' + receivedData.eventType);
                        console.log('M/W2 deviceType : ' + receivedData.deviceType);
                        console.log('M/W2 dataType : ' + receivedData.dataType);
                        console.log('M/W2 data : ' + receivedData.data);
                        console.log('M/W2 targetId : ' + receivedData.data.targetId);
                        console.log('M/W2 controlType : ' + receivedData.data.controlType);
                        console.log('M/W2 controlValue : ' + receivedData.data.controlValue);

                        let sendData = {
                            id: receivedData.id,
                            eventType: 'res',
                            dataType: receivedData.dataType,
                            result: 'success',
                            message: ''
                        };

                        console.log("---------------------------------------------");
                        console.log("sendData : " + JSON.stringify(sendData));
                        console.log("---------------------------------------------\n\n");

                        ws.send(JSON.stringify(sendData));

                        break;
                    default:
                }

                break;

        }



        console.log("---------------------------------------------\n\n");
    }

    function sendStatusData() {
        setTimeout(function(){

